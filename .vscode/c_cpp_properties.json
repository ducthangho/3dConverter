{
    "configurations": [
        {
            "name": "Linux",
            "includePath": [
                "${workspaceFolder}/**",
                "${vcpkgRoot}/x64-linux/include",
                "${vcpkgRoot}/x64-linux-dynamic/include"
            ],
            "defines": [],
            "compilerPath": "/usr/bin/gcc",
            "knownCompilers": [
                {
                    "path": "/usr/bin/gcc",
                    "isC": true
                },
                {
                    "path": "/usr/bin/g++",
                    "isC": false
                },
                {
                    "path": "/usr/bin/cpp",
                    "isC": false
                },
                {
                    "path": "/usr/bin/gcc-7",
                    "isC": true
                },
                {
                    "path": "/usr/bin/g++-7",
                    "isC": false
                },
                {
                    "path": "/usr/bin/gcc-5",
                    "isC": true
                },
                {
                    "path": "/usr/bin/g++-5",
                    "isC": false
                },
                {
                    "path": "/usr/bin/gcc",
                    "isC": false
                }
            ],
            "compilerArgs": [],
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "gcc-x64",
            "configurationProvider": "go2sh.cmake-integration",
            "browse": {
                "path": [
                    "${workspaceFolder}/**",
                    "${vcpkgRoot}/x64-linux/include",
                    "${vcpkgRoot}/x64-linux-dynamic/include"
                ],
                "limitSymbolsToIncludedHeaders": true
            },
            "compileCommands": "${workspaceFolder}/build/compile_commands.json"
        }
    ],
    "version": 4
}